{
  "jsdoc": {
    "prefix": ["jsdoc"],
    "body": [
      "/**",
      " * $1",
      " */"
    ],
    "description": "jsdoc"
  },
  "jsdoc inline": {
    "prefix": ["jsdoci"],
    "body": [
      "/** $1 */"
    ],
    "description": "JSDOC inline"
  },
  "dirname": {
    "prefix": ["dirname"],
    "body": [
      "import * as url from 'url';",
      "const __dirname = url.fileURLToPath(new URL('.', import.meta.url));"
    ],
    "description": "Adds __dirname to an ESM file."
  },
  "filename": {
    "prefix": ["filename"],
    "body": [
      "import * as url from 'url';",
      "const __filename = url.fileURLToPath(new URL(import.meta.url));"
    ],
    "description": "Adds __filename to an ESM file."
  }
}
