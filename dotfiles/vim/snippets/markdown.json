{
  "Header Link": {
    "prefix": ["hl"],
    "body": [
      "<h$1 id=\"$2\">",
      "  <a href=\"#$2\">",
      "    ${3:$TM_SELECTED_TEXT}",
      "  </a>",
      "</h$1>",
      "$0"
    ],
    "description": "Header links"
  },
  "Documentation Frontmatter": {
    "prefix": "dfmt",
    "body": [
      "---",
      "title: $1${VIM:expand(\"%:t:r\")}",
      "permalink: /$2${VIM:expand(\"%:t:r\")}/",
      "---"
    ],
    "description": "Documentation frontmatter"
  }
}
